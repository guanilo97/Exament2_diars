<table class="table table-hover">
    <thead>
        <tr>
            <th>Id_Pokemon</th>
            <th>Nombre</th>
            <th>Tipo</th>
            <th>Fecha_Captura</th>
            <th>Imagen</th>
            <th>Opciones</th>
        </tr>

    </thead>
    <tbody>
        @foreach (var item in Model)
        {

        <tr>
            <td>@item.PokemonId</td>
            <td>@item.pokemon.Nombre</td>
            <td>@item.pokemon.TipoId</td>
            <td>@item.Fecha_captura</td>
            <td><img src="@item.pokemon.Imagen" style="width:70px;height:90px" />  </td>
            <td><a href="/capturar/delate?id=@item.Id"> Eliminar</a></td>
            <td><a href="/Home/Index">Capturar otro pokememon</a></td>
        </tr>
        }
    </tbody>
            </table>
